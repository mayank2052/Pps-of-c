#include <stdio.h>

int main() 
{
    int marks[32];
    int i;

    printf("Enter the marks of 32 students:\n");

    for (i = 0; i < 32; i++) {
        printf("Student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    printf("\nMarks of 32 students are:\n");

    for (i = 0; i < 32; i++)
 {
        printf("Student %d: %d\n", i + 1, marks[i]);
    }

    return 0;
}
